<li class="guided-search-item" id="<%= itemId %>" title="<%= itemText %>" data-icon="false" style="margin-left: 0px; margin-right: 0px;">
    <% if ( itemCount != null ) { %>
        <a><img src="images/<%= icon %>" class="ui-li-icon" style="max-width: 28px; max-height: 28px; top: 7px">&nbsp;<%= itemText %>&nbsp;&nbsp;<span id="guidedSearchCount<%= itemId %>" class="ui-li-count" style="left:auto; right:auto;"><%= itemCount %></span></a>
    <% } else { %>
        <a><img src="images/<%= icon %>" class="ui-li-icon" style="max-width: 28px; max-height: 28px; top: 7px">&nbsp;<%= itemText %>&nbsp;&nbsp;<span id="guidedSearchCount<%= itemId %>" class="ui-li-count" style="left:auto; right:auto;"></span></a>
    <% } %>
    <a class="guided-search-clear-button" data-role="button" data-inline="true" data-icon="delete" data-iconpos="notext" data-theme="r"></a>
</li>
