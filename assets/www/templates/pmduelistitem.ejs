<li id="<%= pmDue.webId %>" class="list-item-padding" data-icon="false" style="margin-left: 0px; margin-right: 0px;"><a>
    <div class="ui-grid-c">
        <div class="ui-block-a">
            <% if ( pmDue.equipment ) { %>
                <% if ( pmDue.equipment.serialNumber ) { %>
                    <h3><%= pmDue.equipment.serialNumber %></h3>
                <% } %>
                <p><%= pmDue.equipment.manufacturer %>,&nbsp;<%= pmDue.equipment.productCode %></p>

                <% if( pmDue.equipment.customerEquipmentId ) { %>
                    <p><%= translations.customerId %> <%= pmDue.equipment.customerEquipmentId  %></p>
                <% } %>
            <% } %>
        </div>
        <div class="ui-block-b">
            <h3><%= translations.scheduled %> <%= Localization.formatDateTime( pmDueDate, 'd' ) %></h3>
        </div>
        <div class="ui-block-c" style="width: 40%;">
            <% if ( pmDue.standardJobCode ) { %>
                 <h3><%= pmDue.standardJobCode.notes %> <%= translations.jobCode %> <%= pmDue.standardJobCode.completeJobCode %></h3>
            <% } else { %>
                <h3><%= translations.noAvailableJobCode %></h3>
            <% } %>
            <% if ( !_.isUndefined( pmDue.standardJobCode.description ) ) { %>
                <p><%= pmDue.standardJobCode.description %></p>
            <% } %>
            <% if ( folder ) { %>
                <p><span><%= translations.pmType %></span><span><%= folder.description %></span></p>
            <% } %>
        </div>
        <div class="ui-block-d" style="width: 5%; margin-top: 10px; margin-right: 15px; float: right;">
            <% if ( pmDue.note && pmDue.note.length > 0 ) { %>
            <img src="images/notesicon-up.png" onclick="PmsDue.displayPMSchduleNotes( event, <%= pmDue.webId %> );" style="border: none;"/>
            <% } %>
        </div>
    </div>
</a></li>
