<?xml version='1.0' encoding='utf-8'?>
<widget id="com.leadingedje.RealPhonegapApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>RealPhonegapApp</name>
    <description>Demo application for the Real World Mobile App Development Using Phonegap</description>
    <author email="keith.wedinger@leadingedje.com" href="http://leadingedje.com">Keith Wedinger</author>
    <content src="login.html" />
    <access origin="*" />
    <feature name="LoadURL">
        <param name="android-package" value="com.leadingedje.RealPhonegapApp.LoadURL" />
    </feature>
</widget>
